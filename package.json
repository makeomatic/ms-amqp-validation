{
  "name": "ms-validation",
  "version": "0.5.0",
  "description": "ajv-based validation utils for mservice architecture",
  "main": "./lib/index.js",
  "scripts": {
    "pretest": "npm rebuild",
    "compile": "if [[ $NODE_ENV != \"production\" ]]; then ./node_modules/.bin/babel -d ./lib --optional es7.objectRestSpread,es7.classProperties,es7.decorators ./src; fi",
    "prepublish": "npm run compile",
    "test": "BLUEBIRD_DEBUG=1 ./node_modules/.bin/mocha --require ./test/babelhook.js --bail -R xunit > test-report-$(node -v).xml",
    "test-local": "NODE_ENV=development ./node_modules/.bin/mocha --require ./test/babelhook.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/makeomatic/ms-amqp-validation.git"
  },
  "author": "Vitaly Aminev <v@makeomatic.me>",
  "contributors": [
    "Dmitry Gorbunov <me@tewi.tk> (http://tewi.tk)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makeomatic/ms-amqp-validation/issues"
  },
  "homepage": "https://github.com/makeomatic/ms-amqp-validation#readme",
  "devDependencies": {
    "babel": "^5.8.34",
    "babel-core": "^5.8.33",
    "babel-eslint": "^4.1.5",
    "chai": "^3.4.1",
    "eslint": "^1.8.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-mocha": "^1.0.0",
    "mocha": "^2.3.3"
  },
  "dependencies": {
    "ajv": "^2.0.1",
    "ajv-i18n": "^1.0.0",
    "bluebird": "^3.0.5",
    "callsite": "^1.0.0",
    "common-errors": "^0.5.3",
    "xtend": "^4.0.1"
  }
}
