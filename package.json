  "devDependencies": {
    "babel-core": "^5.8.33",
    "babel-eslint": "^4.1.3",
    "chai": "^3.4.1",
    "eslint": "^1.8.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-mocha": "^1.0.0",
    "mocha": "^2.3.3"
  },
  "dependencies": {
